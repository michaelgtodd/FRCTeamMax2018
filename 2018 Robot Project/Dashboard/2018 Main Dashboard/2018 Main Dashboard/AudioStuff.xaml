<UserControl x:Class="_2018_Main_Dashboard.AudioStuff"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_2018_Main_Dashboard"
             mc:Ignorable="d" 
             d:DesignHeight="75" d:DesignWidth="150">
    <Grid Background="Black">
        <Rectangle Stroke="White" StrokeThickness="4"></Rectangle>
        <TextBlock Name="ThemeTextblock" Foreground="White" TextAlignment="Center" FontSize="18" FontWeight="Bold" Margin="0,1,0,50">
            Theme Select
        </TextBlock>
        <CheckBox Checked ="Mute"  Unchecked= "Play" Margin="130,55,0,0"> </CheckBox>
    </Grid>
</UserControl>
